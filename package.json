{
	"name": "wp-power-course",
	"private": true,
	"version": "0.8.3",
	"type": "module",
	"scripts": {
		"bootstrap": "yarn && composer install --no-interaction",
		"dev": "vite",
		"prebuild": "echo 'todo: remove console'",
		"build": "node --max_old_space_size=25600 ./node_modules/vite/bin/vite.js build && node release/mv-manifest.cjs",
		"build:wp": "node --max_old_space_size=25600 ./node_modules/vite/bin/vite.js --config vite.config-for-wp.ts build",
		"preview": "vite preview",
		"format": "prettier-eslint --write \"js/src/**/*.tsx\"",
		"lint": "npx eslint \"js/src/**/*.{ts,tsx}\" --ext .ts,.tsx --rule \"no-unused-vars: off\" && phpcbf",
		"lint:fix": "npx eslint \"js/src/**/*.{ts,tsx}\" --fix --rule \"no-unused-vars: off\" && phpcbf",
		"release": "env-cmd -f ./.env release-it patch --ci --verbose --config ./release/.release-it.cjs",
		"release:build-only": "env-cmd -f ./.env release-it patch --ci --verbose --config ./release/.release-it.cjs --build-only",
		"release:patch": "env-cmd -f ./.env release-it patch --ci --verbose --config ./release/.release-it.cjs",
		"release:minor": "env-cmd -f ./.env release-it minor --ci --verbose --config ./release/.release-it.cjs",
		"release:major": "env-cmd -f ./.env release-it major --ci --verbose --config ./release/.release-it.cjs",
		"zip": "env-cmd -f ./.env node ./release/zip.cjs",
		"create:release": "env-cmd -f ./.env node ./release/create-release.cjs",
		"sync:version": "wp-update-project-version -s package.json -p plugin.php",
		"replace-string": "node ./replace-string.cjs"
	},
	"dependencies": {
		"@ant-design/charts": "^2.2.3",
		"@ant-design/pro-components": "2.8.1",
		"@ant-design/pro-editor": "1.3.0",
		"@arco-themes/react-easy-email-theme": "^0.0.3",
		"@pdf-lib/fontkit": "^1.1.1",
		"@refinedev/antd": "5.44.0",
		"@refinedev/cli": "2.16.39",
		"@refinedev/core": "4.56.0",
		"@refinedev/inferencer": "4.7.1",
		"@refinedev/react-router-v6": "4.6.0",
		"@refinedev/simple-rest": "5.0.8",
		"@tanstack/react-query": "4.36.1",
		"@tanstack/react-query-devtools": "4",
		"@uidotdev/usehooks": "^2.4.1",
		"@vidstack/react": "^1.12.12",
		"antd-style": "3.7.1",
		"antd-toolkit": "1.2.13",
		"axios": "1.7.7",
		"canvas-confetti": "^1.9.3",
		"clsx": "^2.1.1",
		"hls.js": "1.5.17",
		"j7-easy-email-core": "4.17.0",
		"j7-easy-email-editor": "4.17.0",
		"j7-easy-email-extensions": "4.17.1",
		"jotai": "2.10.1",
		"mjml-browser": "^4.15.3",
		"parse-json": "^8.1.0",
		"pdf-lib": "^1.17.1",
		"query-string": "9.1.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-final-form": "^6.5.9",
		"react-icons": "5.3.0",
		"react-shadow": "^20.6.0",
		"tailwind-merge": "^2.5.4"
	},
	"devDependencies": {
		"@kucrut/vite-for-wp": "^0.8.0",
		"@rollup/plugin-alias": "^5.1.0",
		"@soderlind/wp-project-version-sync": "^2.0.2",
		"@types/lodash-es": "^4.17.12",
		"@types/mjml-browser": "^4.15.0",
		"@types/node": "^20.12.3",
		"@types/react": "^18.2.74",
		"@types/react-dom": "^18.2.23",
		"@typescript-eslint/eslint-plugin": "^7.5.0",
		"@typescript-eslint/parser": "^7.5.0",
		"@vitejs/plugin-react": "^4.2.1",
		"@wordpress/eslint-plugin": "^17.11.0",
		"adm-zip": "^0.5.12",
		"archiver": "^7.0.1",
		"autoprefixer": "^10.4.19",
		"change-case-all": "^2.1.0",
		"daisyui": "^4.12.2",
		"env-cmd": "^10.1.0",
		"eslint": "^8.57.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-standard-with-typescript": "^43.0.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-n": "^16.6.2",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.34.1",
		"jquery": "^3.7.1",
		"postcss": "^8.4.38",
		"postcss-import": "^16.1.0",
		"postcss-nesting": "^12.1.1",
		"prettier": "^3.2.5",
		"prettier-eslint": "^16.3.0",
		"prettier-eslint-cli": "^8.0.1",
		"prettier-plugin-multiline-arrays": "^3.0.4",
		"release-it": "^17.1.1",
		"replace-in-file": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"sass": "^1.72.0",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.3",
		"typescript-eslint": "^7.5.0",
		"vite": "5.4.11",
		"vite-plugin-live-reload": "^3.0.3",
		"vite-plugin-optimizer": "^1.4.3",
		"vite-tsconfig-paths": "^4.3.2",
		"yargs": "^17.7.2",
		"yarn-upgrade-all": "^0.7.4"
	},
	"bin": "./bin/create.cjs"
}
